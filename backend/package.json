    "cors": "2.7.1",
    "ejs": "2.4.1",
    "errorhandler": "1.4.3",
    "express": "^4.16.3",
    "express-jwt": "3.3.0",
    "express-session": "^1.15.6",
    "fs": "0.0.1-security",
